  "dependencies": {
    "axios": "^0.20.0",
    "cors": "^2.8.5",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "if-env": "^1.0.4",
    "nodemon": "^2.0.4"
  }
}
